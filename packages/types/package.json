{
  "name": "@mmi-monorepo/types",
  "version": "0.1.1",
  "description": "This package will export shared types used across the old @codefi/mmi-sdk library",
  "author": "Albert Olive <albertolivecorbella@gmail.com>",
  "homepage": "https://github.com/ConsenSys/mmi-monorepo#readme",
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "directory": "packages/types",
    "url": "git+https://github.com/ConsenSys/mmi-monorepo.git"
